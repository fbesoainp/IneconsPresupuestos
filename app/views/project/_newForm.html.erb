<%= form_with(model: @project, method: :post, local: true) do |form| %>
    <div class="form-group">
        <%=form.label :Nombre, :class => "control-label"%>
        
            <%=form.text_field :name, :class => "form-control"%>
    </div>

    <div class="form-group">
        <%=form.label :Ubicacion, :class => "control-label"%>
        
            <%=form.text_field :location, :class => "form-control"%>
    </div>

    <div class="form-group">
        <%=form.label :Fecha, :class => "control-label"%>
        
            <%=form.date_field :date, :class => "form-control"%>
    </div>

    <div class="form-group">
        <%=form.label :Factor_Materiales, :class => "control-label"%>
        
            <%=form.number_field :materialsFactor, step: :any, :class => "form-control" %>
    </div>

    <div class="form-group">
        <%=form.label :Factor_Mano_de_Obra, :class => "control-label"%>
        
            <%=form.number_field :workForceFactor, step: :any, :class => "form-control" %>
    </div>

    <div class="form-group">
        <%=form.label :Factor_Gastos_Generales, :class => "control-label"%>
        
            <%=form.number_field :generalExpensesFactor, step: :any, :class => "form-control" %>
    </div>

    <div class="form-group">
        <%=form.label :Factor_Utilidad, :class => "control-label"%>
        
            <%=form.number_field :utilitiesFactor, step: :any, :class => "form-control" %>
    </div>

    <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Volver</button>
        <input type="submit" name="" value="Registrar" class="btn btn-primary">
    </div>
<% end %>
